%nav.top-bar{"data-topbar" => ""}
  %ul.title-area
    %li.name= link_to "#{Rails.application.config_for(:settings).app_name}", root_path
    %li.toggle-topbar.menu-icon
      %a{:href => "#"} Menu
  - if current_user
    .top-bar-section
      %ul
        = render 'layouts/navigation_menu'
      %ul.right
        = render 'layouts/navigation_metas'
