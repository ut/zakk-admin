= render partial: "/partials/header"



.row
  .large-12.columns
    %h3
      - # = @page.title

.row
  .large-4.columns
    - @col1_content.each do |c|
      %div.panel
        %h3= c.title
      %div.panel
        %div= c.text


  .large-4.columns

    - @col2_content.each do |c|
      %div.panel
        %h3= c.title
      %div.panel
        %div= c.text


  .large-4.columns

    - @col3_content.each do |c|
      %div.panel
        %h3= c.title
      %div.panel
        %div= c.text

